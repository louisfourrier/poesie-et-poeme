<!-- Schema.org markup for Google+ -->
<meta itemprop="name" content="<%= content_for?(:title) ? yield(:title) : "PUT YOUR DESCRIPTION HERE" %>">
<meta itemprop="description" content="<%= content_for?(:description) ? yield(:description) : "PUT YOUR DESCRIPTION HERE" %>">
<meta name="keywords"  content="<%= content_for?(:keywords) ? yield(:keywords) : "PUT YOUR DESCRIPTION HERE" %>">
<meta itemprop="image" content="http://www.nadvence-beta.com/assets/nadvence-share.png">

<!-- Twitter Card data -->
<meta name="twitter:card" content="summary">
